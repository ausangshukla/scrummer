<div class="">
	<legend>
		<%= @project.name %> : <%= @project.status %>
		<span class="pull-right">
			<% if can?(:manage, @project) %>
			<%= link_to "Manage Team", project_user_mappings_path(:project_id=>@project.id), :class=>"btn btn-primary"   %>
			<%= link_to "Status Report", status_report_project_path(@project), :class=>"btn btn-success"   %>
			<% end %>
		</span>
	</legend>
	
	<table class="table table-bordered">
			
	<tr>
	  <td>Start date:</td>
	  <td><%= @project.start_date %></td>
	  <td>End date:</td>
	  <td><%= @project.end_date %></td>
	</tr>
	
			
	
		
	</table>
</div>

<%= render partial: "/projects/tabs" %>
